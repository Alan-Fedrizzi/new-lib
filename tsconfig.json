{
  "compilerOptions": {
    "baseUrl": "./",
    "rootDir": ".",
    "paths": {
      "react-native-new-lib": ["./src/index"],
      "tokens/backoffice": [
        "./node_modules/templarios-2.0/tokens/index.backoffice"
      ],
      "tokens/congressos": [
        "./node_modules/templarios-2.0/tokens/index.congressos"
      ],
      "tokens/medsoft": ["./node_modules/templarios-2.0/tokens/index.medsoft"],
      "tokens/storybook": [
        "./node_modules/templarios-2.0/tokens/index.storybook"
      ]
    },
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "resolvePackageJsonImports": false,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "verbatimModuleSyntax": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/styled-components/native/dist"
    ]
  }
}
